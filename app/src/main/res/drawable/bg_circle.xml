<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- State when pressed -->
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/light_text_yellow_11" />
        </shape>
    </item>
    <!-- Default state -->
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/light_solid_yellow_9" />
        </shape>
    </item>
</selector>
